{
  "name": "nexus-decorators",
  "version": "0.0.1",
  "main": "dist",
  "module": "dist-esm",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/graphql-nexus/nexus-decorators.git"
  },
  "author": {
    "name": "Tim Griesser",
    "url": "https://github.com/tgriesser"
  },
  "files": [
    "src",
    "dist",
    "dist-esm",
    "LICENSE.md",
    "README.md",
    "yarn.lock"
  ],
  "scripts": {
    "clean": "rm -rf dist && rm -rf dist-esm",
    "prebuild": "yarn clean",
    "prepublish": "yarn clean && yarn build",
    "build": "tsc && tsc --build tsconfig.cjs.json && tsc --build tsconfig.esm.json",
    "watch": "tsc --build tsconfig.cjs.json --watch"
  },
  "license": "MIT",
  "dependencies": {
    "stack-utils": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^12",
    "@types/stack-utils": "^2.0.0",
    "graphql": "^15.5.0",
    "nexus": "^1.1.0-next.16",
    "typescript": "^4.3.2"
  }
}
